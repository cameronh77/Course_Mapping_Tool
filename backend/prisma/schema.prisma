generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Course {
  Course_ID    Int  @id @default(autoincrement())
  Course_Desc  String 
  Expected_Duration Int
  Number_Teaching_Periods Int
}
